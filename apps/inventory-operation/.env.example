# Example environment variables for Inventory Operation app
VITE_API_URL=https://your-api-url.com
VITE_APP_NAME=Inventory Operation

# Supabase Configuration (Required)
# Get these from your Supabase project dashboard: Settings > API
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# App Configuration
VITE_APP_NAME=Inventory Operation
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

# Localization
VITE_DEFAULT_LOCALE=vi-VN
VITE_DEFAULT_CURRENCY=VND
VITE_DEFAULT_TIMEZONE=Asia/Ho_Chi_Minh

# Features
VITE_ENABLE_REALTIME=true
VITE_ENABLE_OFFLINE_MODE=false

# Debug
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info

# Optional: Firebase (for additional features)
# VITE_FIREBASE_API_KEY=your-firebase-api-key
# VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
# VITE_FIREBASE_PROJECT_ID=your-project-id

# Optional: Google Sheets Integration
# VITE_GOOGLE_SHEETS_API_KEY=your-google-sheets-api-key
# VITE_GOOGLE_SHEETS_SPREADSHEET_ID=your-spreadsheet-id
